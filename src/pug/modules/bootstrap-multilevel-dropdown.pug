mixin bootstrapDropdownNavbar(navbarClass,innerClass,logo,ulClass)

    .navbar.navbar-default.navbar-fixed-top(class=navbarClass,role="navigation")
        div(class=innerClass)
            .navbar-header
                button(type="button",class="navbar-toggle",data-toggle="collapse",data-target=".navbar-collapse")
                    span.sr-only Toggle navigation
                    span.icon-bar
                    span.icon-bar
                    span.icon-bar
                a.navbar-brand(href="index.html")
                    img.brand-logo(src=logo)
            .collapse.navbar-collapse
                ul.navbar-right.nav.navbar-nav(class=ulClass)
                    li
                        a(href="#") Home
                    li
                        a(href="#",class="dropdown-toggle",data-toggle="dropdown") Dropdown
                        ul.dropdown-menu.multi-level
                            li
                                a(href="#") Home
                            li
                                a(href="#",class="dropdown-toggle",data-toggle="dropdown") Dropdown
                                ul.dropdown-menu
                                    li
                                        a(href="#") Home
                                    li.dropdown-submenu
                                        a(href="#",class="dropdown-toggle",data-toggle="dropdown") Dropdown
                                        ul.dropdown-menu
                                            li.dropdown-submenu
                                                a(href="#",class="dropdown-toggle",data-toggle="dropdown") Dropdown
                                                ul.dropdown-menu
                                                    li
                                                        a(href="#") Home


